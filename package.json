{
  "name": "webspatial-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:avp": "XR_ENV=avp vite --port 3001",
    "build": "tsc -b && vite build",
    "build:avp": "XR_ENV=avp npm run build",
    "run:avp": "webspatial-builder run --manifest=dist/webspatial/avp/manifest.json --project=dist --base=https://localhost:3001/ --platform=visionos --bundle-id=com.aila.therapy",
    "dev:simulator": "npm run build:avp && npm run run:avp",
    "build:ipa": "npm run build:avp && webspatial-builder build --manifest=dist/webspatial/avp/manifest.json --project=dist/webspatial/avp --base=dist/webspatial/avp --platform=visionos --bundle-id=com.aila.therapy",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/model-viewer": "^4.1.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.0",
    "@react-three/xr": "^5.7.1",
    "@types/three": "^0.181.0",
    "@webspatial/core-sdk": "^1.0.4",
    "@webspatial/react-sdk": "^1.0.4",
    "@webxr-input-profiles/motion-controllers": "^1.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "three": "^0.181.1",
    "webxr-polyfill": "^2.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/webxr": "^0.5.20",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@webspatial/builder": "^1.0.4",
    "@webspatial/platform-visionos": "^1.0.4",
    "@webspatial/vite-plugin": "^0.1.9",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2",
    "vite-plugin-html": "^3.2.2"
  }
}
